<div class="form-container">  <div class="green-bg">    <div class= "s-container">        <div class="item-container">          <h1>Editez le contact d'urgence</h1>        </div>    </div>  </div>  <div class="item-container">    <%= simple_form_for [@user, @contact], :method => :patch do |f| %>    <%= f.input :first_name, as: :string, placeholder: 'Paul', label: 'Prénom', autofocus: true, class: 's-input' %>    <%= f.input :last_name, as: :string, placeholder: 'Lepoulp', label: 'Nom', autofocus: true %>    <div class="phone-number">      <%= f.input :phone_number, as: :string, placeholder: '76 557 53 52', label: 'Téléphone', autofocus: true %>    </div>    <%= f.button :submit, 'Editer le contact', class: 's-button' %>    <% end %>  </div></div>